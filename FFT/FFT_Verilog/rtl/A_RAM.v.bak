module fft_myown(
		clk,
		rst,
		wr_en,
		datain_re,
		datain_im,
		w_re,
		w_im

	);
	input clk;
	input rst;
	input wr_en;
	input [23:0]datain_re;
	input [23:0]datain_im;
	
	
	reg [11:0]cnt;
	reg [23:0]A_re[7:0];
	reg [23:0]A_im[7:0];//A为每级计算结果所储存的数组
	integer i;
	
	always @(posedge clk or negedge rst)begin
		if(!rst)begin
			cnt	<= 'd0;
			for(i=0;i<8;i=i+1)begin
				A_re[i] 	<= 'd0;
				A_im[i]	<= 'd0;	
			end
				
		end
		
		else if(en)begin
		
			if(cnt == 'd8)begin
				cnt <= 'd0;
			end
			
			else begin
				cnt <= cnt + 1'd1;
				A_re[cnt] <= datain_re;
				A_im[cnt] <= datain_im;
			end
			
		end
		
		else begin
			for(i=0;i<8;i=i+1)begin
				A_re[i] 	<= A_re[i];
				A_im[i]	<= A_im[i];	
			end
		end
		
		
	
	end   
	
	
	
	
endmodule 